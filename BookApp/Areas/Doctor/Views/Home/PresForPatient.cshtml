<style>
    .appointments .appointment-list {
        background-color: #fff;
        border: 1px solid #f0f0f0;
        border-radius: 4px;
        display: -webkit-box;
        display: -ms-flexbox;
        display: flex;
        -ms-flex-wrap: wrap;
        flex-wrap: wrap;
        margin-bottom: 20px;
        padding: 20px;
    }
</style>

<!-- Main Wrapper -->
